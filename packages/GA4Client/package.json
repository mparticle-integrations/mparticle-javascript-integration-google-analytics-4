        "shelljs": "0.8.3",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.1",
        "should": "13.2.3",
        "prettier": "^2.4.1",
        "watchify": "^3.11.0"
    },
